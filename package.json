{
  "name": "hive-alpha-toolkit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "concurrently \"yarn run generate:nexus\" \"yarn run generate:genql\"",
    "generate:nexus": "nodemon --exec 'ts-node --esm --require tsconfig-paths/register --transpile-only -P nexus.tsconfig.json pages/api/graphql' --ext 'ts' --watch '*/graphql/**/*.ts'",
    "generate:genql": "nodemon --exec 'genql --schema ./graphql/schema.graphql --output ./graphql/generated/genql'  --watch 'graphql/schema.graphql'"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.6.4",
    "@genql/runtime": "^2.10.0",
    "@mantine/core": "^4.2.5",
    "@mantine/hooks": "^4.2.5",
    "@mantine/next": "^4.2.5",
    "@nivo/core": "^0.78.0",
    "@nivo/line": "^0.79.1",
    "@prisma/client": "^3.15.2",
    "apollo-server-micro": "^3.9.0",
    "ethers": "^5.6.7",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "12.1.6",
    "nexus": "^1.3.0",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@genql/cli": "^2.10.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@graphql-codegen/typescript-react-apollo": "^3.2.14",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^17.0.44",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "concurrently": "^7.2.2",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.18",
    "prettier": "^2.6.2",
    "prisma": "^3.15.2",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "packageManager": "yarn@3.2.1"
}
